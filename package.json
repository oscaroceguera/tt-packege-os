{
  "name": "tt-package-os",
  "version": "1.0.0",
  "description": "A demo package for total t oscar",
  "keywords": [
    "demo",
    "typescript"
  ],
  "homepage": "https://github.com/oscaroceguera/tt-packege-os",
  "bugs": {
    "url": "https://github.com/oscaroceguera/tt-packege-os/issues"
  },
  "author": "oscar oceguera<oscaroceguera@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:oscaroceguera/tt-packege-os.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.8",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  },
  "scripts": {
    "build": "tsup",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "lint": "tsc",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "main": "dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  }
}
